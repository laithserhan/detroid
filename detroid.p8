pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- esa
-- by cow

function _init()
  p = { sprite = 2, x = 72, y = 72, dx = 0, dy = 0, msp = 2, jsp = 3, isgrounded = false, facing = 1, moving = false }
  g = { grav = 0.2 }
  bullet_list = {}
  frame = 0
  camera_x = 0
  camera_y = 0
end

function _update()
  update_bullets()

  -- jump
  if btnp(4) and p.isgrounded then
    p.dy = -p.jsp
  end

  -- before moving save x
	local x_previous = p.x

  -- walk
  p.dx = 0
  p.moving = false
  if btn(0) then --left
    p.dx = -p.msp
    p.facing = -1
    p.moving = true
  end
  if btn(1) then --right
    p.dx = p.msp
    p.facing = 1
    p.moving = true
  end
  p.x += p.dx

  -- check for walls in the
  -- direction we are moving.
  -- (offset based on size)
  local x_offset = 0
  if p.dx > 0 then x_offset = 7 end

  -- look for a wall
  local h = mget((p.x + x_offset) / 8,(p.y + 7) / 8)
  if fget(h,0) then
    p.x = x_previous
    p.moving = false
  end

  -- apply gravity and vsp
  p.dy += g.grav
  p.y += p.dy

  -- check for floors
  local v = mget((p.x + 4) / 8,(p.y + 8) / 8)
  -- assume they are floating
  -- until we see otherwise
  p.isgrounded = false
  -- if moving down then...
  if p.dy >= 0 then
    -- move to solid and stop
    if fget(v, 0) then
      p.y = flr((p.y) / 8) * 8
      p.dy = 0
      p.isgrounded = true
    end
  end

  -- check for ceilings
  v = mget((p.x + 4) / 8,(p.y) / 8)
  -- if moving up then...
  if p.dy <= 0 then
    -- move to solid and stop
    if fget(v, 0) then
      p.y = flr((p.y + 8) / 8) * 8
      p.dy = 0
    end
  end

  frame += 1

  if p.moving and p.isgrounded then -- animate
    if p.sprite > 3 then p.sprite = 2 end
    if frame % 5 == 0 then
      p.sprite += 1
    end
    if p.sprite > 3 then p.sprite = 2 end
  else
    if p.isgrounded then
      p.sprite = 3
    end
    if not p.isgrounded then
      if p.sprite < 4 then p.sprite = 4 end
      if frame % 1 == 0 then
        p.sprite += 1
        if p.sprite > 6 then p.sprite = 4 end
      end
    end
  end
end

function update_bullets()
  if btnp(5) then
    add(bullet_list, { x = p.x, y = p.y, facing = p.facing })
  end

  local bullet_speed = 6
  for bullet in all(bullet_list) do
    for i=0,bullet_speed do
      bullet.x += 1 * bullet.facing
      -- delete on collision
      local h = mget((bullet.x + 4) / 8, (bullet.y + 4) / 8)
      if fget(h, 0) then
        del(bullet_list, bullet)
        screen_shake(2,1)
      end
    end
  end
end

function draw_bullets()
  for bullet in all(bullet_list) do
    if bullet.facing == 1 then
      spr(13, bullet.x, bullet.y)
    else
      spr(13, bullet.x, bullet.y, 1, 1, true)
    end
  end
end

function draw_player()
  if p.facing == 1 then
    spr(p.sprite, p.x, p.y)
  else
    spr(p.sprite, p.x, p.y, 1, 1, true)
  end
end

function screen_shake(radius,length,flip)
  local old_camera_x = camera_x
  local old_camera_y = camera_y
  for i=0,length do
    cls()
    -- duplicate draw function
    map(0, 0, 0, 0, 128, 128)
    draw_player()
    draw_bullets()
    --
    camera_x += rnd(radius*2)-radius
    camera_y += rnd(radius*2)-radius
    camera(camera_x,camera_y)
    if flip == true then flip() end
    camera_x = old_camera_x
    camera_y = old_camera_y
  end
end

function _draw()
  pal()
	pal(6,0)
  cls()
  map(0, 0, 0, 0, 128, 128)
  draw_player()
  draw_bullets()
  camera(camera_x,camera_y)
end


